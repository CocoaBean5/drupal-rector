{
    "name": "mxr576/drupal8-rector",
    "description": "Instant fixes for your Drupal 8 code by using Rector.",
    "type": "library",
    "keywords": [
      "code style",
      "rector",
      "drupal 8",
      "phpcs",
      "ast"
    ],
    "require": {
        "jawira/case-converter": "^1.1",
        "rector/rector": "0.4.4"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^2.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dezső Biczó",
            "email": "mxr576@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Mxr576\\Rector\\": "src"
      }
    },
    "config": {
      "optimize-autoloader": true,
      "update-with-dependencies": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
      "check-style": "php-cs-fixer fix --config=./.php_cs.dist --dry-run",
      "fix-style": "php-cs-fixer fix --config=./.php_cs.dist"
    }
}
